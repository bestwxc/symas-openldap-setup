olcObjectClasses: ( 1.1.2.2.1 NAME 'dgzqOrganizationalUnit'
  DESC 'dgzqOrganizationalUnit'
  SUP organizationalUnit
  MUST (isBranch $ isSubCompany $ enabled)
  MAY ( deptId $ deptName $ deptLevel $ headDeptNo $ headDeptName $ parentDeptNo $ parentDeptName $ deptNoPath $ deptNamePath $ branchNo ))
olcObjectClasses: ( 1.1.2.2.2 NAME 'dgzqOrganizationalRole'
  DESC 'dgzqOrganizationalRole'
  SUP organizationalRole
  MAY member)
olcObjectClasses: ( 1.1.2.2.3 NAME 'dgzqOrganizationalPerson'
  DESC 'dgzqOrganizationalPerson'
  SUP organizationalPerson
  MUST ( enabled $ activeState )
  MAY ( uid $ ftspUserId $ ftspLoginId $ ftspNickName $ oaLoginName $ hrmEmpId $ hrmEmpNo $ hrmEmpName $ deptNo $ crmUserId $ hsufOperatorNo $ bopOperatorNo $ idNo $ branchNo $ mobile $ email ))
